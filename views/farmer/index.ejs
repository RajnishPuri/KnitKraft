      var CERTIFICATES = ["../assets/img/Batch 1.jpg", "../assets/img/Batch 2.jpg"];
     function FillProgressTimeline(i=0){
      i = parseInt(i);
      certDownloadBtn.href = CERTIFICATES[i];
      batchName.innerHTML = BATCHES[i].name + " (Batch "+(i+1)+")";
      var progressTimeline = document.getElementById("progressTimeline");
      progressTimeline.innerHTML = "";
      const progress = BATCHES[i].progress;
      var lastUpdated = BATCHES[i].lastUpdated;
      PROGRESS_ARRAY.forEach((prg,index)=>{
        
        progressTimeline.innerHTML += `
          <div class="item">
            <div type="radio" class="dot ${
              index <= progress ? "bg-danger":"bg-secondary"
            }" ></div>
            <div class="content">
              <h4 class="title">${prg}</h4>
              ${
                index == progress? `<p class="text-danger">${
                  lastUpdated?`Last Updated: ${new Date(parseInt(lastUpdated)).toLocaleString()}`:""
                }</p>`:""
              }
            </div>
          </div>
          `;

      })
    }
    FillProgressTimeline();

    function updateProgress(index){
      $.ajax({
        url: "https://knitkraft.onrender.com/api/updateProgress",
        type: "POST",
        data: {
          progress: index,
          id: getCookie("userID"),
        },
        success: function (data) {
          // set cookie
          document.cookie = `progress=${index};`+document.cookie;
          console.log(data);
          FillProgressTimeline();
        },
        error: function (err) {
          console.log(err);
        },
      });
    }
