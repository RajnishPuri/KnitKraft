        function successLocation(position) {
          setupMap([position.coords.longitude, position.coords.latitude]);
        }
        function errorLocation() {
          setupMap([-2.24, 53.48]);
        }
        function setupMap(center) {
          const map = new mapboxgl.Map({
            container: "map",
            style: "mapbox://styles/mapbox/navigation-day-v1",
            center: center,
            zoom: 11,
          });
          map.addControl(new mapboxgl.NavigationControl(), "top-right");

          const geocoder = new MapboxGeocoder({
            accessToken: mapboxgl.accessToken,
            mapboxgl: mapboxgl,
            placeholder: "Search for transports",
            proximity: {
              longitude: center[0],
              latitude: center[1],
            },
          });

          TRANSPORTS = TRANSPORTS.map((transport) => {
            return {
              ...transport,
              distance: calculateDistance(center, transport.location),
            };
          });
          TRANSPORTS.sort((a, b) => {
            return a.distance - b.distance;
          });
          console.log(TRANSPORTS);
          document
            .getElementById("geocoder-container")
            .appendChild(geocoder.onAdd(map));
          document.getElementById("transportSearch").onkeyup = function (e) {
            if (e.key == "Enter") {
              var first = transportsListFill(TRANSPORTS, e.target.value);
              if (first.location)
                map.flyTo({
                  center: first.location,
                  zoom: 14,
                });
            } else {
              transportsListFill(TRANSPORTS, e.target.value);
            }
          };

          // add markers to map
          TRANSPORTS.forEach((marker) => {
            var dist = calculateDistance(center, marker.location);
            var el = document.createElement("div");
            el.className = "marker";
            new mapboxgl.Marker()
              .setLngLat(marker.location)
              .setPopup(
                new mapboxgl.Popup({ offset: 25 }).setHTML(`
                                  ${marker.name},
                                  ${dist} <br>
                                  <button class="btn btn-primary btn-sm" data-bs-toggle="modal" data-bs-target="#addCardActionSheet">Request</button>
                                `)
              )
              .addTo(map);
          });
          function transportsListFill(TRANSPORTS, searchText = "", mode) {
            var transportsList = document.getElementById("transportsList");
            transportsList.innerHTML = "";
            var FIRST = {};
            TRANSPORTS.forEach((transport, index) => {
              console.log(transport);
              if (
                transport.name
                  .toLowerCase()
                  .includes(searchText.toLowerCase()) ||
                transport.address.city
                  .toLowerCase()
                  .includes(searchText.toLowerCase()) ||
                transport.address.state
                  .toLowerCase()
                  .includes(searchText.toLowerCase()) ||
                transport.address.country
                  .toLowerCase()
                  .includes(searchText.toLowerCase()) ||
                transport.address.pincode
                  .toLowerCase()
                  .includes(searchText.toLowerCase()) ||
                transport.owner.name
                  .toLowerCase()
                  .includes(searchText.toLowerCase()) ||
                transport.owner.mobile
                  .toLowerCase()
                  .includes(searchText.toLowerCase()) ||
                transport.owner.email
                  .toLowerCase()
                  .includes(searchText.toLowerCase()) ||
                transport.description
                  .toLowerCase()
                  .includes(searchText.toLowerCase()) ||
                searchText == ""
              ) {
                if (!FIRST.name) FIRST = transport;
                transportsList.innerHTML += `
                                  <div class="accordion mb-2" id="accordionExample${index}">
                                  <div class="accordion-item">
                                    <h2
                                      class="accordion-header"
                                      style="display: flex; align-items: center; margin-left: 3vw"
                                    >

                                      <button
                                        class="accordion-button collapsed"
                                        type="button"
                                        data-bs-toggle="collapse"
                                        data-bs-target="#accordion00${index}"
                                      >
                                        ${transport.name} - ${transport.distance} km
                                      </button>
                                    </h2>
                                    <div
                                      id="accordion00${index}"
                                      class="accordion-collapse collapse"
                                      data-bs-parent="#accordionExample${index}"
                                    >
                                      <div class="accordion-body">
                                        <p style="text-align: center; margin-top: 1rem">
                                          <img
                                            src="${transport.image}"
                                            alt="transport image"
                                            style=" max-height: 200px"
                                          />
                                        </p>
                                        <strong>Address:</strong> ${transport.address.city}, ${transport.address.state}, ${transport.address.country}, ${transport.address.pincode} <br>
                                        <strong>Owner:</strong> ${transport.owner.name} <br>
                                        <strong>Phone:</strong> ${transport.owner.mobile} <br>
                                        <strong>Email:</strong> ${transport.owner.email} <br>
                                        <strong>Initial Price:</strong> ₹${transport.initialPrice} <br>
                                        <strong>Description:</strong> ${transport.description} <br>

                                        <p style="text-align: center; margin-top: 1rem">
                                          <button class="btn btn-success btn-sm mb-2" style="margin: 0 auto;"
                                          onclick="CHOOSED_TRANSPORT_PRICE = ${transport.initialPrice}; document.getElementById('myModal').style.display = 'flex';"
                                          >Request</button>
                                        </p>
                                      </div>
                                    </div>
                                  </div>
                                </div>
                        `;
              }
            });
            if (!FIRST.location)
              transportsList.innerHTML =
                "<p style='text-align: center'>No results found</p>";
            return FIRST;
          }
          transportsListFill(TRANSPORTS);
        }
        function calculateDistance(center, location) {
          var R = 6371e3; // metres
          var φ1 = (center[1] * Math.PI) / 180; // φ, λ in radians
          var φ2 = (location.lat * Math.PI) / 180;
          var Δφ = ((location.lat - center[1]) * Math.PI) / 180;
          var Δλ = ((location.lng - center[0]) * Math.PI) / 180;

          var a =
            Math.sin(Δφ / 2) * Math.sin(Δφ / 2) +
            Math.cos(φ1) * Math.cos(φ2) * Math.sin(Δλ / 2) * Math.sin(Δλ / 2);
          var c = 2 * Math.atan2(Math.sqrt(a), Math.sqrt(1 - a));

          var d = R * c; // in metres
          console.log(d);
          return Math.round(d / 1000);
        }
