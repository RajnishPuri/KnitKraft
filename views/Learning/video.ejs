    <style>
        * {
            box-sizing: border-box;
            margin: 0;
            padding: 0;
        }

        body {
            font-family: Arial, sans-serif;
            background-color: #f4f4f4;
            padding: 20px;
        }

        .video-slider {
            max-width: 100%;
            overflow: hidden;
        }

        .video-slider .videos {
            display: flex;
            transition: transform 0.5s ease;
        }

        .video-slide {
            flex: 0 0 100%;
            max-width: 100%;
            padding: 0 10px;
            transition: transform 0.5s ease;
        }

        .video-slide video {
            width: 100%;
            height: auto;
            display: block;
        }

        /* Media queries for responsiveness */
        @media (min-width: 768px) {
            .video-slide {
                flex: 0 0 50%;
                max-width: 50%;
            }
        }

        @media (min-width: 992px) {
            .video-slide {
                flex: 0 0 33.333%;
                max-width: 33.333%;
            }
        }
    </style>
