                    <script>
                        // Optional JavaScript for slider functionality (if needed)
                        const videos = document.querySelector(".videos");
                        let isDragging = false;
                        let startPosition = 0;
                        let currentTranslate = 0;
                        let prevTranslate = 0;

                        videos.addEventListener("mousedown", startDrag);
                        videos.addEventListener("mouseup", endDrag);
                        videos.addEventListener("mouseleave", endDrag);
                        videos.addEventListener("mousemove", drag);
                        videos.addEventListener("transitionend", () => {
                            isDragging = false;
                        });

                        function startDrag(event) {
                            if (!isDragging) {
                                startPosition = event.clientX;
                                isDragging = true;
                            }
                        }

                        function drag(event) {
                            if (isDragging) {
                                const currentPosition = event.clientX;
                                currentTranslate = prevTranslate + currentPosition - startPosition;
                                videos.style.transform = `translateX(${currentTranslate}px)`;
                            }
                        }

                        function endDrag() {
                            isDragging = false;
                            prevTranslate = currentTranslate;
                        }
                    </script>

